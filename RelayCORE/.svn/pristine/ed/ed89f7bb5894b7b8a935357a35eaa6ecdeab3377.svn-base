%%Regole per entita generali
rule(1,entita_generale_asserita_mondo(MND,FMND),and([tag(nom(MND),pos(_),FMND),entita_generali_mondo(MND)]),1,1).
rule(2,oggetto_generale_asserito_mondo(MND,FMND),and([tag(ogg(MND),pos(_),FMND),entita_generali_mondo(MND)]),1,1).


%regole per verbi
rule(3,verbo_asserito_mondo(MND,FMND),and([tag(ver(MND),pos(_),FMND),verbo_mondo(MND)]),1,1).


%Regole per entita discriminanti
