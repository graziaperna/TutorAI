%individua le parole di cultura

rule(1,verbo_asserito_cultura(CLT,Y),and([tag(ver(CLT),pos(_),Y),verbo_cultura(CLT)]),1,1).
rule(2,entita_asserita_cultura(CLT,Y),and([tag(nom(CLT),pos(_),Y),entita_cultura(CLT)]),1,1).
rule(3,oggetto_asserito_cultura(CLT,Y),and([tag(ogg(CLT),pos(_),Y),entita_cultura(CLT)]),1,1).

%indivuduazione di eventuali triple di economia nelle frasi
rule(4,relazione_cultura(CLTE1,CLTV2,CLTO3),and([entita_asserita_cultura(CLTE1,Y1),verbo_asserito_cultura(CLTV2,Y1),oggetto_asserito_cultura(CLTO3,Y1)]),1,1).



