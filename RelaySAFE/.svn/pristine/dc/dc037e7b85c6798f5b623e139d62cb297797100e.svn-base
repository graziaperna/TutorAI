package gui;

import javax.swing.JDialog;
import javax.swing.JOptionPane;

public class Errori {
	public static void popuperrore(String errore_debugging,String erro)
	{
		System.err.println(errore_debugging);
		JOptionPane eccezione =  new JOptionPane(errore_debugging, JOptionPane.ERROR_MESSAGE);
		JDialog exc = eccezione.createDialog("ERRORE" );
		exc.setVisible(true);
	}


}
